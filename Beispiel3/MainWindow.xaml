        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Beispiel3"
        mc:Ignorable="d"
        Title="TopSolid Tool Importer" Height="350" Width="525">
    <Grid>
        <TextBox x:Name="filenameBox" HorizontalAlignment="Left" Height="62" Margin="26,150,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="352"/>
        <Button x:Name="browse" Content="Durchsuchen..." HorizontalAlignment="Left" Margin="383,153,0,0" VerticalAlignment="Top" Width="107" Click="browse_Click"/>
        <Button x:Name="import" Content="Importieren" HorizontalAlignment="Left" Margin="209,248,0,0" VerticalAlignment="Top" Width="75" Click="import_Click"/>

    </Grid>
</Window>
